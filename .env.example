# Anthropic API Key for Claude agents
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# HuggingFace token for SAM3 model access (gated model)
# Get your token from: https://huggingface.co/settings/tokens
# Accept SAM3 model terms at: https://huggingface.co/facebook/sam3
HF_TOKEN=your_huggingface_token_here
HUGGING_FACE_HUB_TOKEN=your_huggingface_token_here

# PyTorch settings for macOS compatibility with SAM3
PYTORCH_ENABLE_MPS_FALLBACK=1
